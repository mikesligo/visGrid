package threads;

import java.io.BufferedReader;
import java.io.File;
import java.io.FileReader;

import org.eclipse.emf.common.util.URI;

public class EMFThread {

	public static void main(String[] args){
		File file = new File("exampleplugin/example.visgrid");
		try{
			URI uri = URI.createPlatformResourceURI("exampleplugin/example.visgrid");
			BufferedReader br = new BufferedReader(new FileReader(uri.toFileString()));
			String line;
			while((line = br.readLine()) != null) {
				System.out.println(line);
			}
		} catch (Exception e){
			e.printStackTrace();
		}
	}

	public void run() {

	}
}
