 */
public static String getType(int visualID) {
	return Integer.toString(visualID);
}
	
		/**
 * @generated
 */
public static int getDiagramVisualID(org.eclipse.emf.ecore.EObject domainElement) {
	if (domainElement == null) {
		return -1;
	}
	if (testLink.TestLinkPackage.eINSTANCE.getatop().isSuperTypeOf(domainElement.eClass()) && isDiagram((testLink.atop) domainElement)) {
	return testLink.diagram.edit.parts.AtopEditPart.VISUAL_ID;
}
	return -1;
}
	
		/**
 * @generated
 */
public static int getNodeVisualID(org.eclipse.gmf.runtime.notation.View containerView, org.eclipse.emf.ecore.EObject domainElement) {
	if (domainElement == null) {
		return -1;
	}
	String containerModelID = testLink.diagram.part.TestLinkVisualIDRegistry.getModelID(containerView);
	if (!testLink.diagram.edit.parts.AtopEditPart.MODEL_ID.equals(containerModelID)) { 
		return -1;
	}
	int containerVisualID;
	if (testLink.diagram.edit.parts.AtopEditPart.MODEL_ID.equals(containerModelID)) {
		containerVisualID = testLink.diagram.part.TestLinkVisualIDRegistry.getVisualID(containerView);
	} else {
		if (containerView instanceof org.eclipse.gmf.runtime.notation.Diagram) {
			containerVisualID = testLink.diagram.edit.parts.AtopEditPart.VISUAL_ID;		
		} else {
			return -1;
		}
	}
	switch (containerVisualID) {
			case testLink.diagram.edit.parts.AtopEditPart.VISUAL_ID:
	if (testLink.TestLinkPackage.eINSTANCE.gettwo().isSuperTypeOf(domainElement.eClass())) {
	return testLink.diagram.edit.parts.TwoEditPart.VISUAL_ID;
}
if (testLink.TestLinkPackage.eINSTANCE.getthree().isSuperTypeOf(domainElement.eClass())) {
	return testLink.diagram.edit.parts.ThreeEditPart.VISUAL_ID;
}
if (testLink.TestLinkPackage.eINSTANCE.getone().isSuperTypeOf(domainElement.eClass())) {
	return testLink.diagram.edit.parts.OneEditPart.VISUAL_ID;
}
	break;
					}
	return -1;
}
	
		/**
 * @generated
 */
public static boolean canCreateNode(org.eclipse.gmf.runtime.notation.View containerView, int nodeVisualID) {
	String containerModelID = testLink.diagram.part.TestLinkVisualIDRegistry.getModelID(containerView);
	if (!testLink.diagram.edit.parts.AtopEditPart.MODEL_ID.equals(containerModelID)) { 
		return false;
	}
	int containerVisualID;
	if (testLink.diagram.edit.parts.AtopEditPart.MODEL_ID.equals(containerModelID)) {
		containerVisualID = testLink.diagram.part.TestLinkVisualIDRegistry.getVisualID(containerView);
	} else {
		if (containerView instanceof org.eclipse.gmf.runtime.notation.Diagram) {
			containerVisualID = testLink.diagram.edit.parts.AtopEditPart.VISUAL_ID;		
		} else {
			return false;
		}
	}
	switch (containerVisualID) {
		case testLink.diagram.edit.parts.AtopEditPart.VISUAL_ID:
	if (testLink.diagram.edit.parts.TwoEditPart.VISUAL_ID == nodeVisualID) {
	return true;
}
if (testLink.diagram.edit.parts.ThreeEditPart.VISUAL_ID == nodeVisualID) {
	return true;
}
if (testLink.diagram.edit.parts.OneEditPart.VISUAL_ID == nodeVisualID) {
	return true;
}
	break;
			}
	return false;
}
	
