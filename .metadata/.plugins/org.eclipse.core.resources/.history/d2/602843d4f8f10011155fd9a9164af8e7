/* LiveGraph JavaDoc style sheet */


/* Page background color */
body { background-color: #FFFFFF; color: #000000; font: normal 10pt Arial,Helvetica,sans-serif; }

a { color: #006600; text-decoration:none }
a:hover { color: #008500; text-decoration:underline }

/* Headings */
h1 { font-size: 130% }

/* Table colors */
.TableHeadingColor     { background: #006600; color:#DEFFFF }
.TableSubHeadingColor  { background: #00CE63; color:#DEFFFF }
.TableRowColor         { background: #FFFFFF; color:#000000 }

.TableHeadingColor a { color: #00E000; text-decoration:none }
.TableHeadingColor a:hover { color: #00FF00; text-decoration:underline }

.TableSubHeadingColor a { color: #004100; text-decoration:none }
.TableSubHeadingColor a:hover { color: #002200; text-decoration:underline }

/* Font used in left-hand frame lists */
.FrameTitleFont   { font-size: 100%; font-family: Helvetica, Arial, sans-serif; color:#000000 }
.FrameHeadingFont { font-size: 100%; font-family: Helvetica, Arial, sans-serif; color:#000000 }
.FrameItemFont    { font-size: 100%; font-family: Helvetica, Arial, sans-serif; color:#000000 }

/* Navigation bar fonts and colors */
.NavBarCell1    { background-color:#DEADCE; color:#000000}
.NavBarCell1Rev { background-color:#990000; color:#FFFFFF} 
.NavBarFont1    { font-family: Arial, Helvetica, sans-serif; color:#000000;color:#000000;}
.NavBarFont1Rev { font-family: Arial, Helvetica, sans-serif; color:#FFFFFF;color:#FFFFFF;}

.NavBarCell2    { font-family: Arial, Helvetica, sans-serif; background-color:#FFFFFF; color:#000000}
.NavBarCell3    { font-family: Arial, Helvetica, sans-serif; background-color:#FFFFFF; color:#000000}