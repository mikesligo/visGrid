package visGrid.diagram.preferences;

import org.eclipse.core.runtime.preferences.AbstractPreferenceInitializer;
import org.eclipse.jface.preference.IPreferenceStore;

/**
 * @generated
 */
public class DiagramPreferenceInitializer extends AbstractPreferenceInitializer {

	/**
	 * @generated
	 */
	public void initializeDefaultPreferences() {
		IPreferenceStore store = getPreferenceStore();
		visGrid.diagram.preferences.DiagramGeneralPreferencePage
				.initDefaults(store);
		visGrid.diagram.preferences.DiagramAppearancePreferencePage
				.initDefaults(store);
		visGrid.diagram.preferences.DiagramConnectionsPreferencePage
				.initDefaults(store);
		visGrid.diagram.preferences.DiagramPrintingPreferencePage
				.initDefaults(store);
		visGrid.diagram.preferences.DiagramRulersAndGridPreferencePage
				.initDefaults(store);

	}

	/**
	 * @generated
	 */
	protected IPreferenceStore getPreferenceStore() {
		return visGrid.diagram.part.VisGridDiagramEditorPlugin.getInstance()
				.getPreferenceStore();
	}
}
